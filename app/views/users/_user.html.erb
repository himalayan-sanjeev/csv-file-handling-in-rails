<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Address city:</strong>
    <%= user.address_city %>
  </p>

  <p>
    <strong>Address state:</strong>
    <%= user.address_state %>
  </p>

  <p>
    <strong>Mobile number:</strong>
    <%= user.mobile_number %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Date of birth:</strong>
    <%= user.date_of_birth %>
  </p>

</div>
